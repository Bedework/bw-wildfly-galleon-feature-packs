<?xml version="1.0" encoding="UTF-8" ?>

<index-properties xmlns="http://bedework.org/ns/" type="org.bedework.indexer.IndexPropertiesImpl">
  <account>admin</account>
  <discardMessages>true</discardMessages>
  <indexPublic>true</indexPublic>
  <indexUsers>true</indexUsers>
  <indexerConfig>${org.bedework.config.dir}/opensearch/</indexerConfig>
  <indexerURL>${com.bedework.oschurls}</indexerURL>
  <indexerUser>admin</indexerUser>
  <indexerPw>admin</indexerPw>
  <keyStore>${org.bedework.config.dir}/opensearch/config/osch.keystore</keyStore>
  <keyStorePw>nopassword</keyStorePw>
  <maxEntityThreads>2</maxEntityThreads>
  <maxPrincipalThreads>2</maxPrincipalThreads>
  <skipPaths>*/Deleted</skipPaths>
</index-properties>
